<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>VegFruits</display-name>
  <welcome-file-list>
    <welcome-file>form.html</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>Vendor</param-name>
    <param-value>Mr.Dileep</param-value>
  </context-param>
  <context-param>
    <param-name>Discount</param-name>
    <param-value>2%</param-value>
  </context-param>
  <servlet>
    <description></description>
    <display-name>VegetableServlet</display-name>
    <servlet-name>VegetableServlet</servlet-name>
    <servlet-class>com.sathya.VegetableServlet</servlet-class>
    <init-param>
      <param-name>V1</param-name>
      <param-value>Tomato</param-value>
    </init-param>
    <init-param>
      <param-name>V2</param-name>
      <param-value>Potato</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>VegetableServlet</servlet-name>
    <url-pattern>/VegetableServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>FruitsServlet</display-name>
    <servlet-name>FruitsServlet</servlet-name>
    <servlet-class>com.sathya.FruitsServlet</servlet-class>
    <init-param>
      <param-name>F1</param-name>
      <param-value>Orange</param-value>
    </init-param>
    <init-param>
      <param-name>F2</param-name>
      <param-value>Mango</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>FruitsServlet</servlet-name>
    <url-pattern>/FruitsServlet</url-pattern>
  </servlet-mapping>
</web-app>